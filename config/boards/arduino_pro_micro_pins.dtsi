/ {
	pro_micro_d: connector_d {
		compatible = "arduino-pro-micro";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map
			= < 0 0 &gpio0  8 0>	/* D0      */
			, < 1 0 &gpio0  6 0>	/* D1      */
			, < 2 0 &gpio0 17 0>	/* D2      */
			, < 3 0 &gpio0 20 0>	/* D3      */
			, < 4 0 &gpio0 22 0>	/* D4/A6   */
			, < 5 0 &gpio0 24 0>	/* D5      */
			, < 6 0 &gpio1  0 0>	/* D6/A7   */
			, < 7 0 &gpio0 11 0>	/* D7      */
			, < 8 0 &gpio1  4 0>	/* D8/A8   */ /* ext up key top for left or rotaly for right */
			, < 9 0 &gpio1  6 0>	/* D9/A9   */ /* ext up key low for left or rotaly for right */
			, <10 0 &gpio0  9 0>	/* D10/A10 */ /* ext low key top */
			, <16 0 &gpio0 10 0>	/* D16     */ /* ext low key low */
			, <14 0 &gpio1 11 0>	/* D14     */
			, <15 0 &gpio1 13 0>	/* D15     */
			, <11 0 &gpio1  1 0>	/* extended on nice_nano */
			, <12 0 &gpio1  2 0>	/* extended on nice_nano */
			, <17 0 &gpio1  7 0>	/* extended on nice_nano */
			, <18 0 &gpio0 12 0>	/* extended on nice_nano */
			, <19 0 &gpio0 26 0>	/* extended on nice_nano */
			;
	};

	pro_micro_a: connector_a {
		compatible = "arduino-pro-micro";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map
			= < 0 0 &gpio1 15 0>	/* A0      */
			, < 1 0 &gpio0  2 0>	/* A1      */
			, < 2 0 &gpio0 29 0>	/* A2      */
			, < 3 0 &gpio0 31 0>	/* A3      */
			, < 6 0 &gpio0 22 0>	/* D4/A6   */
			, < 7 0 &gpio1  0 0>	/* D6/A7   */
			, < 8 0 &gpio1  4 0>	/* D8/A8   */
			, < 9 0 &gpio1  6 0>	/* D9/A9   */
			, <10 0 &gpio0  9 0>	/* D10/A10 */
			;
	};
};

